    </mrow>
  </msup>
  <mo stretchy="false">(</mo>
  <mtext>aq</mtext>
  <mo stretchy="false">)</mo>
  <mo>+</mo>
  <msup>
    <mtext>OH</mtext>
    <mrow class="MJX-TeXAtom-ORD">
      <mo>−<!-- − --></mo>
    </mrow>
  </msup>
  <mo stretchy="false">(</mo>
  <mtext>aq</mtext>
  <mo stretchy="false">)</mo>
