  <mstyle mathcolor="blue">
    <msub>
      <mtext>K</mtext>
      <mrow class="MJX-TeXAtom-ORD">
        <mtext>b</mtext>
      </mrow>
    </msub>
  </mstyle>
  <mo>=</mo>
  <mstyle displaystyle="true">
    <mfrac>
      <mrow>
        <msubsup>
          <mtext>[NH</mtext>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>4</mn>
          </mrow>
