<figure class="figure" title=""><div class="title"></div><div class="alternates"><img draggable="false" src="_plone_ignore_/cache/pspictures/7c31f38304b4f08ad9f7ecdafbd0051b.png"/><pre class="pspicture">&lt;code&gt;
              (-4.3,-2)(4.4,1.5)
	      \newrgbcolor{darkgreen}{0 0.39 0}
	      \newrgbcolor{orange}{1 0.27 0}
	      \newrgbcolor{purple}{0.78 0.13 1}
	      \rput(-2.5,0){\textbf{{\red H$_{2}$O(&amp;#8467;)} + {\blue NH$_{3}$(g)}}}
	      \rput(2.5,0){\textbf{{\red NH$_{4}^{+}$(aq)} + {\blue OH$^{-}$(aq)}}}
